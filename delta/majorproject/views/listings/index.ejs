<% layout("/layouts/boilerplate.ejs") %>
    <h3>All listing</h3>
    <br>
    <!-- <form action="/listings/new" method="get"><button>create new listing</button></form> -->
    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
        <%for (listing of alllisting){%>
            <a href="/listings/<%=listing.id%>" class="listing-link">
                <div class="card">
                    <img src="<%=listing.image%>" class="card-img-top" alt="img" style="height: 20rem;" >
                    <div class="card-img-overlay">hello</div>
                    <div class="card-body">
                        <p class="card-text"><b><%=listing.title%></b></p>
                        <p class="card-text">
                            <%=listing.price%>
                        </p>
                    </div>
                </div>
            </a>
            
            <%}%>
    </div>